{
  "name": "baseline-webpage",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=8192 GENERATE_SOURCEMAP=false react-scripts build",
    "start": "react-scripts start",
    "deploy:dev": "yarn build && export AWS_PROFILE=baseline-webpage && yarn serverless deploy --verbose --stage dev --region ap-southeast-2 --param=\"profile=baseline-webpage\"",
    "remove:dev": "export AWS_PROFILE=baseline-webpage && yarn serverless remove --stage dev --region ap-southeast-2 --param=\"profile=baseline-webpage\"",
    "deploy:prod": "yarn build && export AWS_PROFILE=baseline-webpage && yarn serverless deploy --verbose --stage prod --region ap-southeast-2 --param=\"profile=baseline-webpage\"",
    "remove:prod": "export AWS_PROFILE=baseline-webpage && yarn serverless remove --stage prod --region ap-southeast-2 --param=\"profile=baseline-webpage\"",
    "rename": "./scripts/rename.sh",
    "aws:profile": "./scripts/setup-aws-profile.sh baseline-webpage"
  },
  "dependencies": {
    "bootstrap": "5.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1",
    "reactstrap": "9.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-syntax-flow": "7.14.5",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@popperjs/core": "2.10.2",
    "@types/node": "17.0.18",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.3.11",
    "postcss-scss": "4.0.2",
    "prettier": "2.4.1",
    "react-scripts": "5.0.1",
    "sass": "1.43.4",
    "serverless": "3.21.0",
    "serverless-cloudfront-invalidate": "1.11.0",
    "serverless-s3-sync": "3.0.0",
    "stylelint": "14.9.1",
    "stylelint-config-css-modules": "2.3.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "colors": "1.4.0",
    "autoprefixer": "10.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
